<!DOCTYPE html>
<html lang="en">
  <body>
    <h1><%= data.name %></h1>
    <h3>Followers: <%= data.followers %></h3>

    <% for (let post of data.posts) { %>
      <img src="<%= post.image %>" width="200" />
      <p>Likes: <%= post.likes %> | Comments: <%= post.comments %></p>
      <hr/>
    <% } %>
  </body>
</html>